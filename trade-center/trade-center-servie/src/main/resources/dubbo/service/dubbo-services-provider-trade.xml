<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

  <!-- 定义dubbo服务，必填信息有，interface、ref、owner、timeout，其中dubbo服务对应的bean对象不要在这里配置，直接在实现类上添加注解即可 -->
 
  <dubbo:service interface="com.ai.bdex.dataexchange.tradecenter.dubbo.interfaces.gds.IGdsInfoRSV" ref="gdsInfoRSV"
                 owner="zhaoyf7" timeout="3000"/>

  <dubbo:service interface="com.ai.bdex.dataexchange.tradecenter.dubbo.interfaces.page.IPageInfoRSV" ref="iPageInfoRSV"
                 owner="zhaoyf7" timeout="3000"/>
  <dubbo:service interface="com.ai.bdex.dataexchange.tradecenter.dubbo.interfaces.gds.IGdsCatRSV" ref="gdsCatRSV"
                 owner="zhaoyf7" timeout="3000"/>
  <dubbo:service interface="com.ai.bdex.dataexchange.tradecenter.dubbo.interfaces.gds.IGdsLabelRSV" ref="gdsLabelRSV"
                 owner="zhaoyf7" timeout="3000"/>
  <dubbo:service interface="com.ai.bdex.dataexchange.tradecenter.dubbo.interfaces.gds.IGdsInfo2PropRSV" ref="gdsInfo2PropRSV"
                 owner="zhaoyf7" timeout="3000"/>
  <dubbo:service interface="com.ai.bdex.dataexchange.tradecenter.dubbo.interfaces.gds.IGdsSkuRSV" ref="gdsSkuRSV"
                 owner="zhaoyf7" timeout="3000"/>
  <dubbo:service interface="com.ai.bdex.dataexchange.tradecenter.dubbo.interfaces.gds.IGdsInfoQueryRSV" ref="gdsInfoQueryRSV"
                 owner="zhaoyf7" timeout="3000"/>
</beans>